{
    "version": 2,
    "builds": [{
            "src": "index.html",
            "use": "@vercel/static"
        },
        {
            "src": "css/*",
            "use": "@vercel/static"
        },
        {
            "src": "js/*",
            "use": "@vercel/static"
        },
        {
            "src": "imagenes/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        { "handle": "filesystem" }
    ]
}